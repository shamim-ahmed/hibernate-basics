<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>
  <class name="edu.buet.cse.hibernate.lesson14.model.User" table="User">
    <id name="userId" column="id" type="long">
      <generator class="increment"/>
    </id>
    
    <property name="username" type="string" column="name" not-null="true"/>
    <property name="createdDate" type="date" column="createdDate" not-null="true"/>
  </class>
</hibernate-mapping>